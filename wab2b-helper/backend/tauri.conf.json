{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WAB2B Helper",
  "version": "1.0.3",
  "identifier": "com.wab2b.dashboard.helper",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "WAB2B Helper",
        "width": 500,
        "height": 650,
        "resizable": true,
        "fullscreen": false,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": ["wab2b-helper"]
      }
    },
    "updater": {
      "endpoints": [
        "https://api.github.com/repos/Asdmir786/helper-wab2b-dashboard-system/releases/latest"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZBRkRBQTZCN0Q1QjkwNEYKUldSUGtGdDlhNnI5K2xhWktaNE5DU3ZrYkx6QSszdjgxekRGbFppL1lRUVZTditNcTNLODh3enAK"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
