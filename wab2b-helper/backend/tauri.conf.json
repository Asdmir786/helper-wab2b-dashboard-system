{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WAB2B Helper",
  "version": "1.0.4",
  "identifier": "com.wab2b.dashboard.helper",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "WAB2B Helper",
        "width": 500,
        "height": 650,
        "resizable": true,
        "fullscreen": false,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": [
          "wab2b-helper"
        ]
      }
    },
    "updater": {
      "endpoints": [
        "https://api.github.com/repos/Asdmir786/helper-wab2b-dashboard-system/releases/latest"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDE3OEFFNEFFRkM1NERFNDQKUldSRTNsVDhydVNLRjg2RWdJTFY4L3hQR1U2ekhNd3hMVjF2YXFZRWcxdGFkSG9PL1l4QWpSY3QK",
      "signatureType": "file",
      "windows": {
        "publisher": "WAB2B",
        "timestampUrl": "http://timestamp.digicert.com"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": "888c674a56264aed4e3ad3673ce077c2e4bddd9c",
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.sectigo.com"
    }
  }
}
